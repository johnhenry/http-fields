{
  "name": "http-fields",
  "version": "0.0.0",
  "description": "Modern JavaScript library for parsing and serializing HTTP Structured Field Values (RFC 8941 & RFC 9651)",
  "main": "index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "test": "node --test test/*.mjs",
    "test:basic": "node --test test/test.mjs",
    "test:official": "node --test test/official-tests.mjs",
    "test:serialization": "node --test test/serialization-tests.mjs",
    "test:all": "npm run test && npm run test:serialization",
    "test:watch": "node --test --watch test/*.mjs",
    "test:coverage": "node --test --experimental-test-coverage test/*.mjs",
    "build": "node scripts/build.mjs",
    "lint": "eslint *.mjs test/*.mjs",
    "format": "prettier --write *.mjs test/*.mjs",
    "example": "node examples/index.mjs"
  },
  "keywords": [
    "http",
    "headers",
    "structured-fields",
    "rfc8941",
    "rfc9651",
    "parsing",
    "serialization",
    "web-standards",
    "http-fields",
    "structured-headers",
    "javascript",
    "typescript",
    "zero-dependencies"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/http-fields.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/http-fields/issues"
  },
  "homepage": "https://github.com/yourusername/http-fields#readme",
  "files": [
    "index.mjs",
    "index.cjs",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.8.0"
  }
}